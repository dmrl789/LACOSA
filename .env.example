# Core settings
LACOSA_APP_NAME=LACOSA API
LACOSA_ENV=production
LACOSA_DEBUG=false
LACOSA_VERSION=0.1.0
LACOSA_API_PREFIX=/api

# Networking & security
LACOSA_CORS_ORIGINS=https://app.lacosa.example,https://admin.lacosa.example
LACOSA_CORS_ALLOW_CREDENTIALS=false
LACOSA_LOG_LEVEL=INFO

# Auth configuration
LACOSA_SECRET_KEY=replace-with-strong-secret
ACCESS_TOKEN_EXPIRE_MINUTES=60
